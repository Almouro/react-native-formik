{
  "name": "react-native-formik",
  "version": "0.0.0-development",
  "description": "Make the most of your React Native forms with Formik",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/react-native-formik.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10.18"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "commitmsg": "validate-commit-msg",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bamlab/react-native-formik.git"
  },
  "keywords": [
    "react-native",
    "formik",
    "forms"
  ],
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "author": "Almouro <contact@almouro.com> (http://alexandremoureaux.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bamlab/react-native-formik/issues"
  },
  "homepage": "https://github.com/bamlab/react-native-formik#readme",
  "devDependencies": {
    "@babel/runtime": "^7.9.2",
    "@types/jest": "^25.1.4",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "babel-jest": "25.1.0",
    "babel-preset-react-native": "5.0.2",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.11",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-bambi": "^2.0.2",
    "formik": "^2.1.4",
    "husky": "^4.2.3",
    "jest": "25.1.0",
    "metro-react-native-babel-preset": "0.59.0",
    "prettier": "^1.19.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "0.61.5",
    "semantic-release": "^17.0.4",
    "travis-deploy-once": "^5.0.1",
    "tsdx": "^0.13.0",
    "tslib": "^1.11.1",
    "typescript": "^3.8.3",
    "validate-commit-msg": "^2.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "formik": ">= 0.11.x < 2"
  },
  "eslintConfig": {
    "extends": "bambi/native",
    "rules": {
      "no-unused-vars": "error"
    }
  }
}
